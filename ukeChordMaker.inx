<inkscape-extension xmlns="http://www.inkscape.org/namespace/inkscape/extension">
  <_name>Uke Chord Maker</_name>
  <id>es.ukelab.ukeChordMaker</id>
  <dependency type="executable" location="extensions">ukeChordMaker.py</dependency>
